<div class="page-header">
  <%= link_to training_request_polls_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_training_request_poll_path(@training_request_poll), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show training_request_poll</h1>
</div>

<dl class="dl-horizontal">
  <dt>Vote:</dt>
  <dd><%= @training_request_poll.vote %></dd>

  <dt>Comment:</dt>
  <dd><%= @training_request_poll.comment %></dd>

  <dt>Training request:</dt>
  <dd><%= @training_request_poll.training_request %></dd>

  <dt>User:</dt>
  <dd><%= @training_request_poll.user %></dd>

</dl>
